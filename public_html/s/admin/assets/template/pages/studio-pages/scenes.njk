{% extends "layouts/default.njk" %}

{% set base_path = "table-ajax" %}
{% set page_title = "All scenes" %}
{% set page_description = "" %}

{% block main %}
    <div class="content">
        <div class="container-fluid">
            <div class="row">
                <div class="card">
                    <div class="card-header">
                        <h3 class="card--title">All models</h3>
                    </div>
                    <div class="card-content">
                        <table id="example" class="table table-striped table-no-bordered table-hover"
                               cellspacing="0" width="100%" style="width:100%">
                            <thead>
                            <tr>
                                <th>ID</th>
                                <th>First name</th>
                                <th>Last name</th>
                                <th>ZIP / Post code</th>
                                <th>Country</th>
                            </tr>
                            </thead>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
{% endblock %}